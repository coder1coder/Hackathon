{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:u}] [{Level:u3}] {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithExceptionDetails" ]
  },
  "ConnectionStrings": {
    "DefaultConnectionString": "Server=65.108.91.140;Port=5433;Database=Hackathon;User ID=hackathon;Password=hackathon;",
    "Redis": "65.108.91.140:9379,password=redis,abortConnect=false"
  }
}